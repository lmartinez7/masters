z.test <- function(a, mu, sd){
zeta = (mean(a) - mu) / (sd/sqrt(length(a)));
prob = pnorm(abs(zeta),lower=FALSE); #one tail
result = c("z = ",round(zeta,4),", prob. =",round(prob,4));
return(result);
}
a = c(65, 78, 88, 55, 48, 95, 66, 57, 79, 81);
